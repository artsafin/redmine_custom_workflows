<h2><%= link_to l(:trigger_modules), trigger_modules_path %> &#187; <%= @module %></h2>

<%= error_messages_for 'module' %>

<%= labelled_form_for @module, url: trigger_module_path(@module), html: {class: 'trigger_workflows_form'} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_save) %>
<% end %>

<% html_title(l(:trigger_modules), @module, l(:label_administration)) -%>
